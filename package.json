  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "node .",
    "clean": "rimraf ./dist",
    "prebuild": "npm run clean",
    "build": "tsc",
    "dev": "nodemon src/index.ts",
    "openapi:lint": "npx spectral lint src/docs/docs.json --ruleset src/docs/spectral.json",
